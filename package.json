{
  "name": "greenup",
  "version": "1.0.0",
  "description": "Modernized GreenUP platform for finding and purchasing healthy foods.",
  "main": "server.js",
  "scripts": {
    "test": "npm run lint",
    "start": "node server.js",
    "seeds": "node ./seeds/seed.js",
    "dev": "nodemon --watch controllers --watch models --watch utils --watch views --ext js,handlebars server.js",
    "lint": "eslint \"server.js\" \"controllers/**/*.js\" \"models/**/*.js\" \"public/js/**/*.js\" \"utils/**/*.js\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check \"**/*.{js,json,md,handlebars,css}\"",
    "format:write": "prettier --write \"**/*.{js,json,md,handlebars,css}\"",
    "db:migrate": "node ./db/migrate.js up",
    "db:migrate:undo": "node ./db/migrate.js down",
    "db:migrate:status": "node ./db/migrate.js status"
  },
  "keywords": [
    "health",
    "nutrition",
    "node"
  ],
  "author": "Jesus Vazquez",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.0",
    "connect-session-sequelize": "^7.1.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.18.0",
    "mysql2": "^3.9.2",
    "sequelize": "^6.37.1",
    "uuid": "^9.0.1",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "pino": "^9.0.0",
    "pino-http": "^9.0.0",
    "umzug": "^3.3.1"
  },
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/JesusV545/GreenUP.git"
  },
  "bugs": {
    "url": "https://github.com/JesusV545/GreenUP/issues"
  },
  "homepage": "https://github.com/JesusV545/GreenUP#readme",
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.2.5",
    "eslint-config-prettier": "^9.1.0",
    "pino-pretty": "^11.0.0"
  }
}
